  p2: Y ::= 'm'
      compute
        Y[0].s2 = Y[0].i2;
        Y[0].s3 = 1;
      end;
  p3: Y ::= 'n'
      compute
        Y[0].s2 = 2;
        Y[0].s3 = Y[0].i3;
      end;
  p4: X ::= 'm'
      compute
        X[0].s1 = X[0].i1;
      end;
  p5: Z ::= Y
      compute
        Z[0].s4 = Y[0].s3;
        Y[0].i2 = 3;
        Y[0].i3 = Y[0].s2;
      end;      
