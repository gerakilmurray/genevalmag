semantic domain
  op infix  (10, left) +: int, int -> int;
attributes
  s0: syn <int> of {S};
  s1: syn <int> of {X};
  s2: syn <int> of {Y};
  s3: syn <int> of {Y};
  s4: syn <int> of {Z};
  i1: inh <int> of {X};
  i2: inh <int> of {Y};
  i3: inh <int> of {Y};
  
rules
  p1: S ::= X Y Z
      compute        
        S[0].s0 = X[0].s1+Y[0].s2+Y[0].s3+Z[0].s4;
        X[0].i1 = Y[0].s3;
        Y[0].i2 = X[0].s1;
        Y[0].i3 = Y[0].s2;
      end;
